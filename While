my_list=[42, 69, 322, 13, 0, 99, -5, 9, 8, 7, -6, 5]
ind = 0
print('В списке этом списке')
print(my_list)
print('положительные числа: ')
while ind < len(my_list):
    num = my_list[ind]
    ind = ind + 1
    if num == 0:
        continue
    elif num < 0:
        break
    elif ind == len(my_list):
        print(num)
    else:
        print(num)
